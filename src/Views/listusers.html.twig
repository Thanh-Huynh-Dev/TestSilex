{% extends "layout.html.twig" %}

{% block title %}Liste des utilisateurs{% endblock %}

{% block content %}
    <h1>Liste des utilisateurs</h1>
    
    <ul>
        {% for user in users %}
            <li><a href="{{ url('profile', {id: user.id})}}"/>{{ user.firstname }} {{ user.lastname }}</li>
        {% endfor %}
    </ul>
    
{% endblock %}

